{
	"name": "Rymo color scheme override",
	"groups": {
		"background":                { "fg": "flat:white+",    "bg": "flat:black", "attrs": [] },
		"background:divider":        { "fg": "flat:blue",      "bg": "flat:white+",  "attrs": [] },
		"session":                   { "fg": "flat:white+",    "bg": "flat:black", "attrs": ["bold"] },
		"user":                      { "fg": "flat:white+",    "bg": "flat:magenta+", "attrs": ["bold"] },
		"superuser":                 { "fg": "flat:white+",    "bg": "flat:red",    "attrs": ["bold"] },
		"virtualenv":                { "fg": "flat:white+",    "bg": "flat:green",  "attrs": [] },
		"branch":                    { "fg": "flat:cyan+",     "bg": "flat:black", "attrs": [] },
		"branch_dirty":              { "fg": "flat:yellow",    "bg": "flat:black", "attrs": ["bold"] },
		"branch_clean":              { "fg": "flat:green",     "bg": "flat:black", "attrs": [] },
		"branch:divider":            { "fg": "flat:white",     "bg": "flat:black", "attrs": [] },
		"date":                      { "fg": "flat:blue",      "bg": "flat:white+",  "attrs": [] },
		"date:divider":              { "fg": "flat:blue",      "bg": "flat:white+",  "attrs": [] },
		"time":                      { "fg": "flat:blue",      "bg": "flat:white+",  "attrs": [] },
		"time:divider":              { "fg": "flat:blue",      "bg": "flat:white+",  "attrs": [] },
		"uptime":                    { "fg": "flat:blue",      "bg": "flat:white+",  "attrs": [] },
		"cwd":                	     { "fg": "flat:blue",      "bg": "flat:white", "attrs": [] },
		"cwd:current_folder": 	     { "fg": "flat:white+",    "bg": "flat:blue",   "attrs": ["bold"] },
		"cwd:divider":        	     { "fg": "flat:blue",      "bg": "flat:white", "attrs": [] },
		"environment":        	     { "fg": "flat:black",     "bg": "flat:yellow",  "attrs": [] },
		"attached_clients":   	     { "fg": "flat:white+",    "bg": "flat:green",  "attrs": [] },
		"hostname":           	     { "fg": "flat:white+",    "bg": "flat:green",  "attrs": [] },
		"jobnum":             	     { "fg": "flat:yellow",    "bg": "flat:red+", "attrs": [] },
		"exit_fail":          	     { "fg": "flat:white+",    "bg": "flat:red",    "attrs": [] },
		"exit_success":       	     { "fg": "flat:green+",    "bg": "flat:green",  "attrs": [] },
		"mode":               	     { "fg": "flat:yellow",    "bg": "flat:green",  "attrs": ["bold"] },
		"internal_ip":               { "fg": "flat:white+",    "bg": "flat:magenta+", "attrs": [] },
		"external_ip":               { "fg": "flat:magenta",   "bg": "flat:white+",  "attrs": ["bold"] },

		"gitstatus":                 { "fg": "flat:white",     "bg": "flat:black+", "attrs": [] },
		"gitstatus_branch":          { "fg": "flat:white",     "bg": "flat:black+", "attrs": [] },
		"gitstatus_branch_clean":    { "fg": "flat:green",     "bg": "flat:black+", "attrs": [] },
		"gitstatus_branch_dirty":    { "fg": "flat:white",     "bg": "flat:black+", "attrs": [] },
		"gitstatus_branch_detached": { "fg": "flat:magenta",   "bg": "flat:black+", "attrs": [] },
		"gitstatus_behind":          { "fg": "flat:white+",    "bg": "flat:black+", "attrs": [] },
		"gitstatus_ahead":           { "fg": "flat:white+",    "bg": "flat:black+", "attrs": [] },
		"gitstatus_staged":          { "fg": "flat:green+",    "bg": "flat:black+", "attrs": [] },
		"gitstatus_unmerged":        { "fg": "flat:magenta+",  "bg": "flat:black+", "attrs": [] },
		"gitstatus_changed":         { "fg": "flat:red+",      "bg": "flat:black+", "attrs": [] },
		"gitstatus_untracked":       { "fg": "flat:yellow+",   "bg": "flat:black+", "attrs": [] },
		"gitstatus_stashed":         { "fg": "flat:blue+",     "bg": "flat:black+", "attrs": [] },
		"gitstatus:divider":         { "fg": "flat:black+",    "bg": "flat:black+", "attrs": [] }
	}
